    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <%-body%>
      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  <footer class="main-footer">
